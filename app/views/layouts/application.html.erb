<!DOCTYPE html>
<html>
<head>
  <title>Cero</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <section id="main-wrapper" class="theme-red">
    <%= render partial: 'nav/header' %>
    <%= render partial: 'nav/left' %>
    <section id="main-<%=params[:controller] + '-' + params[:action] %>" class="main-content-wrapper">
      <%= yield %>
      <%= render partial: 'nav/footer' %>
    </section>
  </section>
</body>
</html>
